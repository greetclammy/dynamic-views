{
  "name": "dynamic-views",
  "version": "0.9.0",
  "description": "Elegant card views for Bases and Datacore.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json",
    "postversion": "git push origin main --follow-tags --force",
    "release": "npm version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "knip": "knip"
  },
  "knip": {
    "ignore": [
      "archive/**"
    ],
    "ignoreDependencies": [
      "moment"
    ],
    "ignoreExportsUsedInFile": true
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.130",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.79.0",
    "obsidian": "^1.11.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "dependencies": {
    "@panzoom/panzoom": "^4.6.1"
  }
}
